data <- data.frame(cm560 = c(10.35, 13.41, 7.78, 10.40, 17.78, 10.40),
cm720 = c(25.93, 38.63, 25.15, 24.25, 41.45, 29.20),
species = factor(c("SS", "JL", "LP", "SS", "JL", "LP")),
nutrient = factor(c("+", "+", "+", "-", "-", "-")))
#MANOVAS
result_manova <- manova(cbind(cm560, cm720) ~ species + nutrient, data = data)
summary(result_manova)
#Two-way ANOVA
result_anova <- aov(cbind(cm560, cm720) ~ Species + Nutrient, data = data)
summary(result_anova)
#Two-way ANOVA
result_anova <- aov(cbind(cm560, cm720) ~ Species + Nutrient, data = data)
